<title>Change Password</title>
</head>

<body>
  {{>navigation}} {{>nonscript}}
  <div class="view-account-password-container">


    <div class="view-account-password-old">
      <div class="box-title">
        Old Password
      </div>
      <div class="box-content flex-center">
        <input type="text" />
      </div>
    </div>
    <div class="view-account-password-new">
      <div class="box-title">
        New Password
      </div>
      <div class="box-content flex-center">
        <input type="text" />
      </div>
    </div>
    <div class="view-account-password-confirm">
      <div class="box-title">
        Confirm New Password
      </div>
      <div class="box-content flex-center">
        <input type="text" />
      </div>
    </div>
    <div id="btns-container" class="change-password-btns">
      <div class="btn flex-center border-right-black" onclick="backtopage();">
        Cancel
      </div>
      <div class="btn flex-center border-right-black" onclick="backtopage();">
        Save
      </div>
    </div>
  </div>
</body>
