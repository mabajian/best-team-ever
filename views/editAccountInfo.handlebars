<title>Edit Account Info</title>
</head>
<body>
  {{>navigation}}
  {{>nonscript}}
  <div class="view-account-info-container">
    <div class="view-account-info-email">
      <div class="box-title">
          Email
      </div>

      <div class="border-black view-account-info-text flex-center">
        <input class="edit-account-inputs" type="text" name="" placeholder="john@example.com">

      </div>
    </div>
    <div class="view-account-info-username">
      <div class="box-title">
          Username
      </div>

      <div class="border-black view-account-info-text flex-center">
          <input class="edit-account-inputs" type="text" name="" placeholder="johndoe">

      </div>
    </div>
    <div class="view-account-info-dob">
      <div class="box-title">
          Date of birth
      </div>

      <div class="border-black view-account-info-text flex-center">
        <input class="edit-account-inputs" type="date" name="" placeholder="Jan 1, 1957">

      </div>
    </div>
  <div id="btns-container" class="edit-account-info-btns">
    <div class="btn flex-center border-right-black" onclick="backtopage();">
      Cancel
    </div>
    <div class="btn flex-center" onclick="backtopage();">
      Save
    </div>
  </div>

</body>
